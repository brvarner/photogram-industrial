<p style="color: green"><%= notice %></p>

<h1>Photos</h1>

<div id="photos">
  <% @photos.each do |photo| %>
    <%= render photo %>
    <p>
      <%= link_to "Show this photo", photo %>
    </p>
  <% end %>
</div>

<%= link_to "Edit.#{current_user.id}", edit_user_registration_path(current_user) %>

<%= link_to "New photo", new_photo_path %>
